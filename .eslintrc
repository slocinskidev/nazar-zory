{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "no-shadow": "off",
    "no-unused-vars": "off",
    "react/prop-types": 0,
    "react/forbid-prop-types": 0
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "window": false,
    "document": false
  }
}
